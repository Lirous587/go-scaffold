package {{.Domain}}

import (
	"github.com/gin-gonic/gin"
)

func RegisterV1(r *gin.RouterGroup, ctrl *Controller) bool {
	g := r.Group("/v1/{{.Domain}}")
	{
        g.POST("/", ctrl.Create{{.DomainTitle}})
        g.DELETE("/:id", ctrl.Delete{{.DomainTitle}})
		g.PUT("/:id", ctrl.Update{{.DomainTitle}})
	}
	return true
}